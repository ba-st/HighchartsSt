Class {
	#name : #HighchartsPlotOptionsVectorStatesSelectTest,
	#superclass : #TestCase,
	#category : #'HighchartsSt-V8-Series-Vector-Tests'
}

{ #category : #'tests-accessing' }
HighchartsPlotOptionsVectorStatesSelectTest >> testGetAnimation [

	| component javascript expectedConfiguration |

	component := HighchartsPlotOptionsVectorStatesSelect new.
	component animation.
	javascript := String streamContents: [ :stream | component javascriptContentOn: stream ].
	expectedConfiguration := Dictionary new
		at: 'animation' put: Dictionary new;
		yourself.
	self assert: javascript equals: expectedConfiguration asJavascript
]

