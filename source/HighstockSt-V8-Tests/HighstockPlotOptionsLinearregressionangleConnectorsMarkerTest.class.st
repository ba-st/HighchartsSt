Class {
	#name : #HighstockPlotOptionsLinearregressionangleConnectorsMarkerTest,
	#superclass : #TestCase,
	#category : #'HighstockSt-V8-Tests'
}

{ #category : #'tests-accessing' }
HighstockPlotOptionsLinearregressionangleConnectorsMarkerTest >> testAlign [

	| component javascript expectedConfiguration |

	component := HighstockPlotOptionsLinearregressionangleConnectorsMarker new.
	component align: 'test'.
	javascript := String streamContents: [ :stream | component javascriptContentOn: stream ].
	expectedConfiguration := Dictionary new
		at: 'align' put: 'test';
		yourself.
	self assert: javascript equals: expectedConfiguration asJavascript
]

