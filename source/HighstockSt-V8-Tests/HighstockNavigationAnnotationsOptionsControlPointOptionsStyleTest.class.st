Class {
	#name : #HighstockNavigationAnnotationsOptionsControlPointOptionsStyleTest,
	#superclass : #TestCase,
	#category : #'HighstockSt-V8-Tests'
}

{ #category : #'tests-accessing' }
HighstockNavigationAnnotationsOptionsControlPointOptionsStyleTest >> testSetFill [

	| component javascript expectedConfiguration |

	component := HighstockNavigationAnnotationsOptionsControlPointOptionsStyle new.
	component fill: 'test'.
	javascript := String streamContents: [ :stream | component javascriptContentOn: stream ].
	expectedConfiguration := Dictionary new
		at: 'fill' put: 'test';
		yourself.
	self assert: javascript equals: expectedConfiguration asJavascript
]

