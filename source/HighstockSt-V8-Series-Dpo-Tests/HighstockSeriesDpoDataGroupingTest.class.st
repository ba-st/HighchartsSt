Class {
	#name : #HighstockSeriesDpoDataGroupingTest,
	#superclass : #TestCase,
	#category : #'HighstockSt-V8-Series-Dpo-Tests'
}

{ #category : #'tests-accessing' }
HighstockSeriesDpoDataGroupingTest >> testSetApproximation [

	| component javascript expectedConfiguration |

	component := HighstockSeriesDpoDataGrouping new.
	component approximation: 'test'.
	javascript := String streamContents: [ :stream | component javascriptContentOn: stream ].
	expectedConfiguration := Dictionary new
		at: 'approximation' put: 'test';
		yourself.
	self assert: javascript equals: expectedConfiguration asJavascript
]

