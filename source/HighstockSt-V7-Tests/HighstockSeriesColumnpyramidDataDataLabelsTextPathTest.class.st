Class {
	#name : #HighstockSeriesColumnpyramidDataDataLabelsTextPathTest,
	#superclass : #TestCase,
	#category : #'HighstockSt-V7-Tests'
}

{ #category : #'tests-accessing' }
HighstockSeriesColumnpyramidDataDataLabelsTextPathTest >> testAttributes [

	| component javascript expectedConfiguration |

	component := HighstockSeriesColumnpyramidDataDataLabelsTextPath new.
	component attributes: 'test'.
	javascript := String streamContents: [ :stream | component javascriptContentOn: stream ].
	expectedConfiguration := Dictionary new
		at: 'attributes' put: 'test';
		yourself.
	self assert: javascript equals: expectedConfiguration asJavascript
]

{ #category : #'tests-accessing' }
HighstockSeriesColumnpyramidDataDataLabelsTextPathTest >> testEnabled [

	| component javascript expectedConfiguration |

	component := HighstockSeriesColumnpyramidDataDataLabelsTextPath new.
	component enabled: 'test'.
	javascript := String streamContents: [ :stream | component javascriptContentOn: stream ].
	expectedConfiguration := Dictionary new
		at: 'enabled' put: 'test';
		yourself.
	self assert: javascript equals: expectedConfiguration asJavascript
]
