Class {
	#name : #HighstockPlotOptionsSeriesDataLabelsTextPathTest,
	#superclass : #TestCase,
	#category : #'HighstockSt-V7-Tests'
}

{ #category : #'tests-accessing' }
HighstockPlotOptionsSeriesDataLabelsTextPathTest >> testAttributes [

	| component javascript expectedConfiguration |

	component := HighstockPlotOptionsSeriesDataLabelsTextPath new.
	component attributes: 'test'.
	javascript := String streamContents: [ :stream | component javascriptContentOn: stream ].
	expectedConfiguration := Dictionary new
		at: 'attributes' put: 'test';
		yourself.
	self assert: javascript equals: expectedConfiguration asJavascript
]

{ #category : #'tests-accessing' }
HighstockPlotOptionsSeriesDataLabelsTextPathTest >> testEnabled [

	| component javascript expectedConfiguration |

	component := HighstockPlotOptionsSeriesDataLabelsTextPath new.
	component enabled: 'test'.
	javascript := String streamContents: [ :stream | component javascriptContentOn: stream ].
	expectedConfiguration := Dictionary new
		at: 'enabled' put: 'test';
		yourself.
	self assert: javascript equals: expectedConfiguration asJavascript
]
