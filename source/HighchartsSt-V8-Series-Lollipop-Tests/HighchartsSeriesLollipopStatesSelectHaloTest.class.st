Class {
	#name : #HighchartsSeriesLollipopStatesSelectHaloTest,
	#superclass : #TestCase,
	#category : #'HighchartsSt-V8-Series-Lollipop-Tests'
}

{ #category : #'tests-accessing' }
HighchartsSeriesLollipopStatesSelectHaloTest >> testSetAttributes [

	| component javascript expectedConfiguration |

	component := HighchartsSeriesLollipopStatesSelectHalo new.
	component attributes: 'test'.
	javascript := String streamContents: [ :stream | component javascriptContentOn: stream ].
	expectedConfiguration := Dictionary new
		at: 'attributes' put: 'test';
		yourself.
	self assert: javascript equals: expectedConfiguration asJavascript
]

