baseline
baseline: spec
	<baseline>
	spec
		for: #common
		do: [ spec
				project: 'XMLParser'
				with: [ spec
						className: 'ConfigurationOfXMLParser';
						versionString: #stable;
						loads: #('Core' 'Tests');
						repository: 'http://smalltalkhub.com/mc/PharoExtras/XMLParser/main' ].
			spec
				package: 'HighchartsStCore';
				package: 'HighchartsStAutoGenerated' with: [ spec requires: #('HighchartsStCore') ];
				package: 'HighstockStAutoGenerated'
					with: [ spec requires: #('HighchartsStAutoGenerated' 'HighchartsStCore') ];
				package: 'HighchartsStExtensions'
					with:
						[ spec requires: #('HighchartsStCore' 'HighchartsStAutoGenerated' 'HighstockStAutoGenerated') ];
				package: 'HighchartsStAutoGeneratedTests'
					with: [ spec requires: #('HighchartsStAutoGenerated') ];
				package: 'HighstockStAutoGeneratedTests'
					with: [ spec requires: #('HighstockStAutoGenerated') ];
				package: 'HighchartsStExamples' with: [ spec requires: #('HighchartsStExtensions') ];
				package: 'HighchartsStTools' with: [ spec requires: #('HighchartsStCore' 'XMLParser') ].
			spec
				group: 'default' with: #('Core' 'HighchartsStExamples');
				group: 'Core'
					with:
					#('HighchartsStCore' 'HighchartsStAutoGenerated' 'HighstockStAutoGenerated' 'HighchartsStExtensions');
				group: 'Tests' with: #('HighchartsStAutoGeneratedTests' 'HighstockStAutoGeneratedTests');
				group: 'Development' with: #('Core' 'Tests' 'HighchartsStExamples');
				group: 'Tools' with: #('HighchartsStTools') ]